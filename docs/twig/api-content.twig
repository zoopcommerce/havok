        <div data-dojo-type="docs/ScopeToggle">
          <button>Privates</button>
          <button>Inherited</button>
        </div>
    </div>
    <w-tree store="apiTree" class="well" item-icon="" {% if location %}active="{{location}}"{% endif %}></w-tree>
